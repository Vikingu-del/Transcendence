# Application settings
VUE_APP_ENV=development
VUE_APP_VERSION=1.0.0

# Backend communication
VUE_APP_API_GATEWAY_URL=http://api-gateway.example.com
VUE_APP_AUTH_SERVICE_URL=http://api-gateway.example.com/authenticationService

# Security settings
VUE_APP_AUTH_TOKEN_LIFESPAN=3600
VUE_APP_JWT_SECRET=your_jwt_secret_key
VUE_APP_COOKIE_SECURE=false

# Frontend config
VUE_APP_BASE_URL=https://frontend.example.com
VUE_APP_DEBUG=true
VUE_APP_LOG_LEVEL=info

# Feature flags
VUE_APP_ENABLE_CHAT=true
VUE_APP_ENABLE_NOTIFICATIONS=true
VUE_APP_ENABLE_ANALYTICS=false

# Third-party integrations
VUE_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
VUE_APP_SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0
VUE_APP_ENABLE_SENTRY=true

# WebSocket URLs
VUE_APP_CHAT_WS_URL=ws://api-gateway.example.com/chatService/ws
VUE_APP_GAME_WS_URL=ws://api-gateway.example.com/gameService/ws

# Locale and timezone
VUE_APP_DEFAULT_LANGUAGE=en
VUE_APP_TIMEZONE=UTC

# Rate limiting and timeouts
VUE_APP_API_REQUEST_TIMEOUT=5000
VUE_APP_MAX_RETRY_ATTEMPTS=3
VUE_APP_RATE_LIMIT_REQUESTS=100